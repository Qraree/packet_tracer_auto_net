<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.core.gui.controllers.DeviceCardController" spacing="10" alignment="CENTER_LEFT">
    <ImageView fx:id="deviceImage" fitWidth="50" fitHeight="50"/>
    <VBox>
        <Label fx:id="deviceName" style="-fx-font-weight: bold;"/>
        <Label fx:id="deviceDescription"/>
    </VBox>
</HBox>